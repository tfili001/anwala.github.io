      Google.defineSlot('/1095769/300x250_DK5_4-5', [300, 250], 'div-gpt-ad-1444077860311-5').addService(googletag.pubads());
      Google.defineSlot('/1095769/300x250_DK5_7-8', [300, 250], 'div-gpt-ad-1444077860311-6').addService(googletag.pubads());
    }
    if (DailyKos.Ads.isStoryPage()) {
      Google.defineSlot('/1095769/300x250_Story_DK5_ATF', [[300, 250], [300, 600]], 'div-gpt-ad-1449600715015-0').addService(googletag.pubads());
      Google.defineSlot('/1095769/300x250_Story_DK5_ATF', [[300, 250], [300, 600]], 'div-gpt-ad-1449600715016-0').addService(googletag.pubads());
      Google.defineSlot('/1095769/300x250_DK5_Story_BTF', [[300, 600], [300, 250]], 'div-gpt-ad-1450740708411-0').addService(googletag.pubads());
    }
    Google.defineOutOfPageSlot('/1095769/1x1_DK5_Interstitial', 'div-gpt-ad-1446660743951-0').addService(googletag.pubads())
      .setForceSafeFrame(false);
    Google.defineSlot('/1095769/PROD_Desktop_Stripe', [1, 1], 'div-gpt-ad-1474507278922-0').addService(googletag.pubads())
      .setForceSafeFrame(false);
    Google.defineSlot('/1095769/970x90_DK_ATF', [[1, 1], [728, 90], [970, 250], [970, 90]], 'div-gpt-ad-1516846012089-0').addService(googletag.pubads())
      .setForceSafeFrame(false);
  };
  
  DailyKos.Ads.Google.defineSlotMobile = function defineSlotMobile () {
    var Google = DK.Ads.Google.init();
  
    Google.defineSlot('/1095769/PROD_Mobile_Petitions', [1, 1], 'div-gpt-ad-1474507097776-0').addService(googletag.pubads())
      .setForceSafeFrame(false);
  };
</script>

