    </span>
    <div class='remove-comment-wrapper requires-moderator pull-right'>
      <a class='remove-comment comment-only' data-remove="comment">Remove Comment</a>
      <b> | </b>
      <a class='remove-comment comment-children' data-remove="children">+ Children</a>
    </div>
    <div class='comment-wrapper media {{wrapperClasses}}'>
      <a class='pull-left author-avatar' href="/user/{{author}}">
        {{#ifUnreadComment this}}
          <span class='new_comment_indicator'>
            <span class='new_indicator_text'>[new]</span>
            <span class='dot dkons primitive-dot'></span>
          </span>
        {{/ifUnreadComment}}
        <img src="{{author_avatar}}" class="media-object avatar">
      </a>
      <span class="hidden-comment-controls">
  <a href='#' class="dkons icon-eye toggle-hidden-comments" title='See hidden comments'></a> HIDDEN COMMENT
</span>

      <div class="comment-body-wrapper">
        <div class='media-body comment-inner'>
          <div class='meta comment-row'>
            <div class='comment-cell'>
              <div class='comment-author pull-left'>
                <span class="author-name"><a href="/user/{{author}}" id="{{author_id}}">{{author}}</a></span>
                {{#if in_reply_to}}
                  <span class="reply-arrow dkons triangle-right"></span>
                  <a href="#comment_{{parent_id}}" class='parent-author'>{{in_reply_to}}</a>
                {{/if}}
              </div>
              <a href="{{commentPath id}}" class='comment-time pull-left'>{{{created_at}}}</a>
            </div>
          </div> <!-- .meta.heading -->
          <div class='comment-row'>
            <div class='comment-text comment-cell'>
              <span class='dk5_subject_text'>{{{preprocessSubjectText subject_text}}}</span>
              <span class='comment-title'>{{{subject}}}</span>
              {{{insertEmbeds textToRender}}}
            </div>
            <div class='comment-cell'></div>
          </div>
          <div class='meta comment-row'>
            <div class="comment-actions pull-left">
              {{#unless preview}}
                <span class="actions heading pull-left">
                  {{> commentActions}}
                </span> <!-- .actions.heading -->
              {{/unless}}
            </div> <!-- .comment-actions.pull-left -->
          </div> <!-- .meta.heading -->
        </div> <!-- .media-body -->
      </div> <!-- .comment-body-wrapper -->
    </div>
    {{#if preview}}
      <div class="comment-buttons clearfix">
        <button class="post enabled btn btn-default reply-button pull-right">Submit Your Comment</button>
        <p class='pull-right cancel-link'>
          <a class='edit' href='#'>&laquo; Return to edit</a>
        </p>
      </div>
    {{/if}}
  </div>
</script>

<script type="text/javascript">
$(function(){
  // almost all of this, except maybe an init line, gets moved to
  // stories.js when we merge in epics/story_authoring_tool,
  // which is why they are hanging out and not namespaced
  var $comments =  $("#comments");

  _.defer(function(){ CommentApp.initHiddenCommentsClickListener($comments); });

  DailyKos.Story.initStoryPageAndLoadComments({
    el:               $comments,
    tree:             $(".comment-tree"),
    storyID:          1741355,
    authorID:         132426,
    commentsCloseAt:  new Date(1519774358000),
    commentsPath:     '/stories/1741355/comments'
  });
});
</script>


