<script>
  (function () {
    (function() {
      DailyKos.Ads.push('div-gpt-ad-1446660743951-0', '.interstitial-ad');
  
      initAd({
        $iframeContainer: $('.interstitial-ad'),
        showAd: function () { $('body').addClass('show-interstitial');    },
        hideAd: function () { $('body').removeClass('show-interstitial'); },
        onLoad: function () { }
      })
    })();
  
    (function() {
      $(document).ready(function() {
        DailyKos.Ads.push('div-gpt-ad-1474507278922-0', '.desktop-stripe-ad');
  
        initAd({
          $iframeContainer: $('.desktop-stripe-ad'),
          showAd: function () {
            $('.desktop-stripe-ad').show();
            var width = _.max([$('.desktop-stripe-ad').parent().width(), 850]);
            $('.desktop-stripe-ad').removeAttr('style');
            $('.desktop-stripe-ad').css({'width': width, 'z-index': 99999});
            $('.desktop-stripe-ad iframe')[0].contentWindow.$('html').css({width: '100%', height: 50, 'z-index': 99999});
            $('.desktop-stripe-ad iframe')[0].contentWindow.$('body').css({width: '100%', height: 50, 'z-index': 99999});
            $('.desktop-stripe-ad iframe')[0].contentWindow.$('#ad').css({width: '100%', height: 50, 'z-index': 99999});
            $('.desktop-stripe-ad iframe')[0].contentWindow.$('#ad .ad-inner').css({width: '100%', height: 50, 'z-index': 99999});
  
            $(window).on('resize.adResize', function() {
              $('.desktop-stripe-ad').css({ 'width': _.max([$('.desktop-stripe-ad').parent().width(), 850]) });
            });
  
          },
          hideAd: function () { $('.desktop-stripe-ad').hide(); }
        });
      });
    })();
  
    function initAd(ad) {
      ad.$iframeContainer
        .find('iframe:first')
        .load(function() {
          App.Ads.Splash.init(ad);
        });
    }
  })();
</script>

