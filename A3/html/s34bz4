      function refresboxes()
      {
              //window resize
            var currentWidth = 1100;
           
              var winWidth = $(window).width();
              var conWidth;
              
              if(winWidth < 480) {
                col = 1;
              } else if(winWidth < 767) {
                col = 2;
              }else if( winWidth < 979) {
                col = 3;
              }else if(winWidth > 979){
                col = 4;
              }
              
              if(conWidth != currentWidth) {
                currentWidth = conWidth;
                $('#container').width(conWidth);
                $('#container').BlocksIt({
                  numOfCol: col,
                  offsetX: 8,
                  offsetY: 8
                });
              }
            
      }



    });
</script>

