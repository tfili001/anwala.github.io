<script>
          P.when("A").execute("books-entity-teaser", function (A) { 
              var elem = A.$("#books-entity-teaser");
              function scrollObserver() {
                  if (A.onScreen(elem, 400)) {
                      A.off("scroll", scrollObserver);
                      A.get("/gp/product/features/entity-teaser/books-entity-teaser-ajax.html?ASIN=0881068365&PRODUCT_GROUP=book_display_on_website", {
                          cache: true,
                          success: function(data) {
                              elem.html(data); 
                          }
                      }); 
                   }
               }
               A.on("scroll", scrollObserver); 
          });

</script>
